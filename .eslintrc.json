{
  "extends": "airbnb-base",
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "plugins": ["json"],
  "rules": {
    "comma-dangle": ["error", "never"],
    "func-names": 0,
    "function-paren-newline": 0,
    "max-len": 0,
    "no-param-reassign": 0,
    "no-var": 0,
    "no-underscore-dangle": 0,
    "object-shorthand": 0,
    "prefer-arrow-callback": 0,
    "prefer-template": 0,
    "space-before-function-paren": 0
  }
}
